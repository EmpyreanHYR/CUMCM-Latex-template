@article{LSTM的能量预测算法,
	title = {{基于ARIMA}-{LSTM的能量预测算法}},
	volume = {49},
	issn = {1003-3114},
	url = {https://kns.cnki.net/KCMS/detail/detail.aspx?dbcode=CJFD&dbname=CJFDAUTO&filename=WXDT202301019&v=},
	abstract = {无线传感器网络的节点运行往往受限于能量供给。对太阳能进行采集并转换成电能存储，可以延长节点的使用寿命。对太阳能进行能量预测，可以更好地规划和使用采集到的能量，这有助于节省能源、避免浪费，提升无线传感器网络的生存周期。针对太阳能预测，提出一种基于自回归积分移动平均-长短期记忆(Autoregressive Integrated Moving Average-Long Short Term Memory, ARIMA-LSTM)组合模型的能量预测方法。首先，采用ARIMA模型来对太阳辐照数据进行预测，提取数据中的线性分量；然后将过滤后的残差代入LSTM神经网络模型，得到非线性分量的预测；最后将二者...},
	language = {en},
	number = {1},
	urldate = {2023-02-11},
	journal = {无线电通信技术},
	author = {{沈露露,梁嘉,乐周}},
	year = {2023},
	keywords = {能量预测, 自回归积分移动平均算法, ARIMA algorithm, energy prediction, LSTM algorithm, LSTM算法},
	pages = {150--156},
}

@article{ARIMA组合模型,
	title = {{基于GM}(1,1)-{ARIMA组合模型的宁波港口物流需求预测分析}},
	volume = {45},
	issn = {1674-4993},
	url = {https://kns.cnki.net/KCMS/detail/detail.aspx?dbcode=CJFD&dbname=CJFDAUTO&filename=SPCY202301009&v=},
	abstract = {港口在我国物流业发展中起着重要的作用，也对社会经济的发展有着促进作用。港口物流业有着很大的潜力和发展空间，对港口物流的需求进行预测是港口物流发展优化过程中重要的一部分。文中将两种单项预测模型，即灰色关联预测与ARIMA模型预测进行组合，形成组合预测模型，从而对港口物流进行预测。预测结果表明，研究的预测结果对宁波港口物流的发展有一定的参考作用，为港口物流需求预测提供了一定的方法和手段。},
	language = {zh},
	number = {1},
	urldate = {2023-02-11},
	journal = {物流工程与管理},
	author = {{张婉琳}},
	year = {2023},
	keywords = {港口物流需求, 灰色关联预测, 回归预测, 组合预测模型, ARIMA prediction, combined forecasting model, grey correlation prediction, port logistics demand},
	pages = {32--35},
}
